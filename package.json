{
  "name": "vue-blood-donation",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-debug": "vavite-loader vite dev",
    "build": "set NODE_ENV=production && vavite build",
    "serve": "node -r dotenv/config dist/server/index.mjs",
    "seed": "node --loader  ts-node/esm --experimental-specifier-resolution=node -r dotenv/config ./api/seed.ts",
    "db": "surreal start --log trace --bind 0.0.0.0:8080 file:api/db",
    "preview": "vite preview",
    "zrok": "~/zrok/zrok share public localhost:3000 --headless",
    "start-zrok": "npm run build && concurrently \"npm run serve\" \" npm run zrok\"",
    "start": "npm run build && npm run serve",
    "visualize": "npx vite-bundle-visualizer -c vite.configvisualize.ts"
  },
  "dependencies": {
    "vue": "^3.3.9",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@mdi/font": "^7.3.67",
    "@netlify/functions": "^2.4.0",
    "@phosphor-icons/web": "^2.0.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/compiler-sfc": "^3.3.9",
    "@vue/server-renderer": "^3.3.9",
    "@vueuse/core": "^10.6.1",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.16.1",
    "lottie-web-vue": "^2.0.7",
    "phosphor-css": "^3.0.0",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.41.1",
    "sass": "^1.69.5",
    "sirv": "^2.0.3",
    "sitemap": "^7.1.1",
    "surrealdb.js": "^0.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vavite": "^3.0.0",
    "vike": "^0.4.147",
    "vite": "^4.0.2",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-i18n": "^9.8.0",
    "vue-tsc": "^1.8.22",
    "vuetify": "^3.4.3",
    "@intlify/vue-i18n-extensions": "^5.0.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/promise-fs": "^2.1.5",
    "@vavite/node-loader": "^3.0.0",
    "pnpm": "^8.11.0",
    "prettier": "3.1.0",
    "vite-plugin-sitemap": "^0.5.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18"
  }
}
