{
  "name": "vue-blood-donation",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vavite build",
    "serve": "node -r dotenv/config dist/server/index.mjs",
    "seed": "node --loader  ts-node/esm --experimental-specifier-resolution=node  -r dotenv/config ./api/seed.ts",
    "db": "surreal start --log trace -u root -p root --bind 0.0.0.0:8080 file:api/db",
    "preview": "vite preview",
    "zrok": "~/zrok/zrok share public localhost:3000 --headless",
    "start-zrok": "npm run build && concurrently \"npm run serve\" \" npm run zrok\"",
    "start": "npm run build && npm run serve",
    "visualize": "npx vite-bundle-visualizer -c vite.configvisualize.ts"
  },
  "dependencies": {
    "@fawmi/vue-google-maps": "^0.9.79",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@mdi/font": "^7.2.96",
    "@netlify/functions": "^2.0.2",
    "@phosphor-icons/web": "^2.0.3",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vueuse/core": "^10.4.1",
    "compression": "^1.7.4",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fp-ts": "^2.16.1",
    "lottie-web-vue": "^2.0.7",
    "phosphor-css": "^3.0.0",
    "pinia": "^2.1.6",
    "primeicons": "^6.0.1",
    "primevue": "^3.34.1",
    "sass": "^1.66.1",
    "sirv": "^2.0.3",
    "surrealdb.js": "^0.9.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vavite": "^3.0.0",
    "vike": "^0.4.142",
    "vike-vue": "^0.3.0",
    "vite": "^4.4.9",
    "vite-plugin-compression2": "^0.10.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-i18n": "^9.5.0",
    "vue-tsc": "^1.8.10",
    "vuetify": "^3.3.17"
  },
  "devDependencies": {
    "@intlify/vue-i18n-extensions": "^4.0.0",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "@types/promise-fs": "^2.1.2",
    "prettier": "3.0.3",
    "vite-plugin-sitemap": "^0.5.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18"
  }
}
